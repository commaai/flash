    "@vitest/coverage-v8": "3.2.1",
    "@vitest/ui": "3.2.1",
    "autoprefixer": "10.4.21",
    "eslint": "9.28.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-svelte": "3.9.1",
    "happy-dom": "17.6.3",
    "jsdom": "26.1.0",
    "postcss": "8.5.4",
    "prettier": "3.5.3",
    "prettier-plugin-svelte": "3.4.0",
    "stylelint": "16.20.0",
    "stylelint-config-standard": "38.0.0",
    "svelte": "5.33.14",
    "svelte-check": "4.2.1",
    "tailwindcss": "4.1.8",
    "typescript": "5.8.3",
